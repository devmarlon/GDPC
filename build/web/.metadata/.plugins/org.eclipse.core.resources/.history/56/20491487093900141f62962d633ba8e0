	<queryString language="SQL">
		<![CDATA[SELECT 
    e . *, p.PAR_RAZAO, fc.CAT_NOME, m.*
FROM
    Equipamento e
        INNER JOIN
    Parceiro p ON (e.PAR_ID = p.PAR_ID)
        JOIN
    gdpc.FactoryCategoria fc ON (e.EQP_CATID = fc.CAT_ID)
    JOIN Modelo m ON (m.MOD_ID = e.MOD_ID)
WHERE
    e.ativo = 1]]>
	</queryString>
