	<queryString language="SQL">
		<![CDATA[SELECT 
    e . *, p.PAR_RAZAO, fc.CAT_NOME
FROM
    Equipamento e
        INNER JOIN
    Parceiro p ON (e.PAR_ID = p.PAR_ID)
        JOIN
    gdpc.factorycategoria fc ON (e.EQP_CATID = fc.CAT_ID)
WHERE
    e.ativo = 1]]>
	</queryString>
