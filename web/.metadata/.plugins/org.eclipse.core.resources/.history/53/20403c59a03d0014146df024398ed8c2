	<queryString language="SQL">
		<![CDATA[SELECT 
    mit . *, s . *, p . *
FROM
    MOVIMENTOITEM mit
        LEFT JOIN
    Servico s ON mit.servico_SRV_ID = s.SRV_ID
        LEFT JOIN
    Peca p ON mit.peca_PEC_ID = p.PEC_ID
WHERE
    mit.MOV_ID = $P{MOV_ID}]]>
