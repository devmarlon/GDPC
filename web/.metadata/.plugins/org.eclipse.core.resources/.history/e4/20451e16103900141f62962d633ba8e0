	<queryString language="SQL">
		<![CDATA[SELECT 
    *
FROM
    EquipamentoServico es
        LEFT JOIN
    Servico s ON (es.SRV_ID = s.SRV_ID)
        LEFT JOIN
    Movimento m ON (es.MOV_ID = m.MOV_ID)
WHERE
    EQP_ID = $P{EQP_ID}
 AND 
 	es.REALIZADO = TRUE
ORDER BY es.MANUTATUAL , es.SRV_ID]]>
	</queryString>
