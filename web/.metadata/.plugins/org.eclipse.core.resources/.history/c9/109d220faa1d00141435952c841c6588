	<queryString language="SQL">
		<![CDATA[select 
    p .PAR_ID,
    p.PAR_RAZAO,
	p.VEN_ID,
	p.PAR_CIDADE,
	p.PAR_UF
from
    Parceiro p
    WHERE 1=1
    AND IF($P{PAR}  <> "", p.PAR_RAZAO LIKE CONCAT("%", $P{PAR}, "%"), 1)
    AND IF($P{FUN_ID} <> "0", p.VEN_ID = $P{FUN_ID} , 1)
    AND IF($P{MUN} <> "0", p.PAR_CIDADE = $P{MUN} , 1)
    AND IF($P{UF} <> "0", p.PAR_UF = $P{UF} , 1)
   
order by p.PAR_RAZAO]]>
	</queryString>
