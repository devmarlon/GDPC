	<queryString language="SQL">
		<![CDATA[SELECT 
    p.codigo, p.descricao, sum(ps.quantidade) as quantidade
FROM
    PecaEqs ps
        inner join
    EquipamentoServico es ON ps.ID_EQS = es.ID_EQS
        inner join
    Peca p ON ps.PEC_ID = p.PEC_ID
WHERE
    EQP_ID =  $P{EQPID} 
group by ps.PEC_ID]]>
